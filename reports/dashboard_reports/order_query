select code, timestamp, login, name, client_code, due_date, po_number, start_date, classification, completion_date, billed_date, charged_amount, bid_offer_price, client_rep, sales_rep, closed, revenue_month, invoiced_date, actual_cost, expected_cost, price, client_login, client_bid, expected_revenue_month, expected_delivery_date, expected_price, platform, needs_completion_review from "order" where s_status is NULL;
